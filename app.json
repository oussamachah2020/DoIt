{
  "expo": {
    "name": "todo",
    "slug": "todo",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/notification-icon.png",
    "scheme": "com.supabase",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#F5F5F5",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffff"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#002C56"
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.todo.app"
    },
    "android": {
      "package": "com.todo.app",
      "googleServicesFile": "./google-services.json",
      "extra": {
        "eas": {
          "projectId": "73c7643b-2093-4ca4-899d-c2c0139c0f92"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",

        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#002C56"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "73c7643b-2093-4ca4-899d-c2c0139c0f92"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/73c7643b-2093-4ca4-899d-c2c0139c0f92"
    }
  }
}
